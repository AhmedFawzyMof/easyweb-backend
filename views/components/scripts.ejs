<script>
  if (document.cookie.indexOf("visited=true") === -1) {
    document.cookie = "visited=true";
    window.location.reload();
  }
  document.addEventListener("DOMContentLoaded", () => {
    const cookies = document.cookie.split(";");
    if (cookies[0].split("=")[1] === "ar") {
      document.querySelectorAll("#sh-rtl").forEach((el) => {
        el.style.direction = "rtl";
      });
      document.querySelectorAll(".sh-rtl").forEach((el) => {
        el.style.direction = "rtl";
      });
    }
    const hero = document.querySelector(".hero-wrap");
    const container = hero.querySelector("#wrap");

    if (window.outerWidth >= 1440) {
      container.classList.remove("container-fluid");
      container.classList.add("container");
    }

    document.addEventListener("click", (e) => {
      const prevActive = document.querySelector(".nav-item.active");
      if (e.target.parentNode.classList.contains("nav-item")) {
        if (e.target.parentNode === prevActive) return;
        if (prevActive) prevActive.classList.remove("active");
        e.target.parentNode.classList.add("active");
      }
    });
  });
</script>
